{
  "name": "betrix-ui",
  "version": "2.0.0",
  "type": "module",
  "description": "BETRIX - Global Sports AI Platform",
  "main": "src/worker-final.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon --watch src --exec node src/app.js",
    "worker": "node src/worker-final.js",
    "bot:start": "node src/bot/server.js",
    "lint": "eslint 'src/**/*.{js,mjs}' --max-warnings=0",
    "format": "prettier --write 'src/**/*.{js,json,md}'",
    "test": "node --test ./tests/*.test.js || echo 'no tests'",
    "docker:build": "docker build -t adminoroo/betrix-ui-new:latest .",
    "redis:validate": "node scripts/validate-redis-connection.js",
    "redis:monitor": "node scripts/monitor-redis-health.js",
    "redis:test-handlers": "node scripts/test-redis-handlers.js",
    "redis:health": "npm run redis:validate && npm run redis:test-handlers"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.30.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@supabase/supabase-js": "^2.26.0",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.20.4",
    "bull": "^4.16.5",
    "bullmq": "^5.64.1",
    "chalk": "^5.6.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^4.22.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "telegraf": "^4.16.3",
    "twilio": "^5.10.6",
    "ws": "^8.18.3"
  },
  "keywords": [
    "telegram",
    "bot",
    "sports",
    "ai",
    "betting",
    "global"
  ],
  "author": "BETRIX Team",
  "license": "MIT",
  "bin": {
    "betrix-ui": "bin/start-server.js"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maryreaky/betrix-ui-replit-.git"
  },
  "bugs": {
    "url": "https://github.com/maryreaky/betrix-ui-replit-/issues"
  },
  "homepage": "https://github.com/maryreaky/betrix-ui-replit-#readme"
}
